<h1>Contacts#index</h1>
<p>Find me in app/views/contacts/index.html.erb</p>

<%= form_tag contact_path do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>
  <div>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </div>
  <div>
    <%= label_tag :text %>
    <%= text_field_tag :text %>
  </div>
  <%= submit_tag 'Contact'%>
<% end %>