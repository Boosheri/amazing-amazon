<!DOCTYPE html>
<html>
  <head>
    <title>AmazingAmazonMarch2019</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav>
      <a href="/">Home</a>
      <%= link_to 'Home', home_path, {class: 'hi'} %>
      |
      <%= link_to 'About', about_path %>
      |
      <%= link_to 'Contact Us', contact_path %>
    </nav>
    <%= yield %>
  </body>
</html>
